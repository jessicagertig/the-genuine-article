    "@types/node": "^18.16.0",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "emotion-theming": "^11.0.0",
    "framer-motion": "^10.12.12",
